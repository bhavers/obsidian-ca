{
    "compilerOptions": {
        "baseUrl": ".",
        "inlineSources": true,
        "module": "ESNext",
        "target": "ES2022",
        "allowJs": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "importHelpers": true,
        "isolatedModules": true,
        "strictNullChecks": true,
        "lib": ["DOM", "ES2022", "ESNext"],
        "types": ["obsidian-typings"]
    },
    "extends": "@tsconfig/svelte/tsconfig.json",
    "include": ["**/globals.d.ts", "**/*.js", "**/*.svelte", "**/*.ts"]
}
